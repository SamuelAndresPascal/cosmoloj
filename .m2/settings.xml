<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
    xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <id>deploy-local</id>
      <username>${DEPLOY_USER}</username>
      <password>${DEPLOY_PASSWORD}</password>
    </server>
    <server>
      <id>site-local</id>
      <username>${SITE_USER}</username>
      <password>${SITE_PASSWORD}</password>
    </server>
    <server>
      <id>deploy-public</id>
      <username>${PUBLIC_DEPLOY_USER}</username>
      <password>${PUBLIC_DEPLOY_PASSWORD}</password>
    </server>
    <server>
      <id>site-public</id>
      <username>${PUBLIC_SITE_USER}</username>
      <password>${PUBLIC_SITE_PASSWORD}</password>
    </server>
    <server>
      <id>github</id>
      <username>${GITHUB_USERNAME}</username>
      <password>${GITHUB_PACKAGE_TOKEN}</password>
    </server>
  </servers>
</settings>